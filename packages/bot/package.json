{
  "name": "@rollem/bot",
  "version": "2.9.6",
  "description": "Rollem (Discord Bot) - Bot Deployable",
  "author": "David <npm@davidsharer.com> (http://twitter.com/david_does)",
  "homepage": "https://github.com/rollem-discord/rollem-discord",
  "bugs": {
    "url": "https://github.com/rollem-discord/rollem-discord/issues"
  },
  "scripts": {
    "clean": "rm -rf ./dist",
    "watch": "yarn watch:bot",
    "watch:bot": "cross-env NODE_ENV=development NODE_OPTIONS=\"--inspect --openssl-legacy-provider\" webpack --watch",
    "build": "yarn run clean && yarn run build:bot",
    "build:bot": "cross-env NODE_ENV=development NODE_OPTIONS=\"--inspect --openssl-legacy-provider\" webpack",
    "build:dockerfile": "cross-env NODE_ENV=development NODE_OPTIONS=\"--inspect --openssl-legacy-provider\" webpack",
    "dev": "yarn run dev:bot",
    "dev:bot": "cross-env NODE_ENV=development NODE_OPTIONS=\"--inspect --openssl-legacy-provider\" webpack --watch",
    "start": "yarn run start:bot",
    "start:bot": "yarn run webpack:launch",
    "webpack:launch": "cross-env NODE_ENV=development NODE_OPTIONS=\"--inspect --openssl-legacy-provider\" nodemon -r dotenv/config dist/bundle.js dotenv_config_path=../../secrets/vscode.env",
    "package": "yarn run package:bot",
    "package:bot": "docker build -t rollem-bot .",
    "package:start": "yarn run package:bot:start",
    "package:bot:start": "docker run --env-file ../../secrets/vscode.env rollem-bot",
    "test": "mocha -r ts-node/register -r tsconfig-paths/register src/**/*.spec.ts",
    "typeorm": "nodemon -r dotenv/config -r ts-node/register ./node_modules/typeorm/cli.js",
    "package:start:fake": "node -r dotenv/config dist/bundle.js dotenv_config_path=../../secrets/vscode.env",
    "start-for-dockerfile": "node dist/bundle.js",
    "old:terraform": ".\\infra\\terraform\\scripts\\xxx-bash-docker-windows-proxy.sh run -it -v `pwd`infra/terraform/clusters:/app/ -w /app/ hashicorp/terraform:light",
    "old:start": "node -r dotenv/config ./rollem-dist/rollem-bot/bot.js dotenv_config_path=./secrets/vscode.env",
    "old:start-for-dockerfile": "node rollem-dist/rollem-bot/bot.js",
    "old:update-k8s-credentials": "doctl kubernetes cluster kubeconfig save rollem-115-sfo2",
    "old:replace-rollem-beta": "kubectl replace -f ../infra/k8s/rollem-beta/",
    "old:replace-rollem-prod": "kubectl replace -f ../infra/k8s/rollem-prod/",
    "old:delete-evicted-pods": "kubectl get pods | grep Evicted | awk '{print $1}' | xargs kubectl delete pod",
    "old:forward-grafana": "/bin/bash \\<\\<\\< 'kubectl port-forward $(kubectl get  pods --selector=app=grafana --output=jsonpath=\"{.items..metadata.name}\") 3000'",
    "old:start-sharder": "node -r dotenv/config ./rollem-dist/rollem-bot/sharder.js dotenv_config_path=./secrets/vscode.env",
    "old:start-package": "docker run --env-file ./secrets/vscode.env rollem-discord",
    "old:build": "gulp build",
    "old:watch": "gulp watch"
  },
  "files": [
    "./"
  ],
  "mocha": {
    "require": [
      "ts-node/register",
      "tsconfig-paths/register"
    ],
    "spec": "src/**/*.spec.ts",
    "extension": [
      "ts"
    ]
  },
  "dependencies": {
    "@rollem/common": "^2.7.0",
    "@rollem/language": "^2.7.2",
    "@types/chai": "^4.2.11",
    "@types/chance": "^1.1.0",
    "@types/express": "^4.17.13",
    "@types/keyv": "^3.1.1",
    "@types/lodash": "^4.14.136",
    "@types/mocha": "^7.0.2",
    "@types/node": "^16.9.1",
    "applicationinsights": "^2.3.6",
    "chai": "^3.5.0",
    "chance": "^1.1.5",
    "del": "^3.0.0",
    "discord.js": "^14.7.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "ffmpeg-static": "^4.3.0",
    "gulp": "^4.0.0",
    "gulp-ext-replace": "^0.3.0",
    "gulp-pegjs": "^0.2.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-typescript": "^5.0.0-alpha.3",
    "gulp-watch": "^4.3.11",
    "injection-js": "^2.2.1",
    "lodash": "^4.17.15",
    "mocha": "^3.2.0",
    "module-alias": "^2.2.0",
    "moment": "^2.22.1",
    "node-cache": "^5.1.0",
    "npm": "^6.3.0",
    "nyc": "^15.0.1",
    "pegjs": "0.10.0",
    "pegjs-loader": "^0.5.6",
    "pg": "^8.3.3",
    "prom-client": "^14.1.0",
    "reflect-metadata": "^0.1.13",
    "ts-loader": "^8.1.0",
    "ts-mocha": "^7.0.0",
    "ts-node": "^10.1.0",
    "ts-pegjs": "^0.2.6",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.3",
    "typeorm": "^0.2.31",
    "typescript": "^4.6.3",
    "watchify": "^3.9.0",
    "webpack": "^5.47.1",
    "webpack-cli": "^4.7.2",
    "webpack-filter-warnings-plugin": "^1.2.1",
    "webpack-node-externals": "^3.0.0",
    "webpack-shell-plugin-next": "^2.2.2"
  },
  "resolutions": {
    "node-fetch": "3.0.0-beta.9"
  },
  "license": "MIT",
  "gitHead": "b36c5f893eec7c328f6d00170c24774db3ee6878",
  "devDependencies": {
    "copy-webpack-plugin": "^8.1.0",
    "cross-env": "^7.0.3"
  }
}
